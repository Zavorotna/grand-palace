  body(tabindex="0")
    .body-wrapper
      include ../components/vars
      include components/mixins
      include components/header

      main.container-galery.container
        .galety-header
            h1.documentation__title.title.title--one-word
                span.head-line Галерея
        .button-block
            button.galery-description__btn.btn(href="№") Фото
            button.galery-description__btn.btn(href="№") Видео
        .construction-progress-page
            .construction-progress__galery-block.row
                .construction-progress__img-galery
                  img(src="../img/construction-progress1.jpeg" alt="Grand Palace - хід будівництва")
                .construction-progress__img-galery
                  img(src="../img/construction-progress2.jpeg" alt="Grand Palace - хід будівництва")
                .construction-progress__img-galery
                  img(src="../img/construction-progress3.jpeg" alt="Grand Palace - хід будівництва")
                .construction-progress__img-galery
                  img(src="../img/construction-progress4.jpeg" alt="Grand Palace - хід будівництва")

    script(src="js/scripts.js")
    include components/footer

