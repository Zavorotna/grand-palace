    meta(name="publisher" content="Grand Palace")
    meta(name="robots" content="index follow")  
    link(rel='preconnect' href='https://fonts.googleapis.com')
    link(rel='preconnect' href='https://fonts.gstatic.com' crossorigin='')
    link(href='https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@400;500;600&family=Roboto:wght@400;500&display=swap' rel='stylesheet')
    link(rel='icon' href='img/favicon.ico')
    link(rel='canonical' href='https://www.grandpalace.km.ua/commercial.html')
    meta(property="og:title" content="Grand Palace - комерційна нерухомість у місті Хмельницький")
