    meta(property="og:image" content="img/logo.jpg")
    meta(property="og:type" content="website")
    meta(property="og:url" content="https://www.grandpalace.km.ua/commercial.html")

  body(tabindex="0")
    .body-wrapper
      include components/vars
      - var location = "/"
      include components/mixins
      include components/header
      main.main
        include components/commercial-hero
        include components/commercial-overview
        include components/commercial-advantages
        include components/commercial-plan

      include components/footer
    script(src="js/scripts.js" defer)
